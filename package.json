    "@tailwindcss/postcss": "^4.1.4",
    "@types/jasmine": "~6.0.0",
    "@types/semver": "^7.5.8",
    "autoprefixer": "^10.4.20",
    "jasmine-core": "~6.0.0",
    "jsdom": "^28.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.3.0",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.9.0",
    "vitest": "^4.0.17"
  }
